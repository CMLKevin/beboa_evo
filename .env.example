# Discord Bot Configuration
# Get these from https://discord.com/developers/applications

# Bot token from Discord Developer Portal
DISCORD_TOKEN=your_bot_token_here

# Your Discord application's client ID
CLIENT_ID=your_client_id_here

# Your Discord server (guild) ID
GUILD_ID=your_guild_id_here

# Channel IDs
# The #log-in channel where /checkin is allowed
CHECKIN_CHANNEL_ID=your_checkin_channel_id_here

# The #beboas-command-center channel for reward notifications
NOTIFICATION_CHANNEL_ID=your_notification_channel_id_here

# Role ID to ping when rewards are redeemed (e.g., @bebebebebebe)
ADMIN_ROLE_ID=your_admin_role_id_here
